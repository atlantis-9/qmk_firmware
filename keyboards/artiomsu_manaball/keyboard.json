{
    "keyboard_name": "ArtiomSu ManaBall",
    "url": "https://github.com/ArtiomSu/qmk_firmware",
    "maintainer": "ArtiomSu",
    "manufacturer": "Artiom",
    "processor": "RP2040",
    "bootloader": "rp2040",
    "usb": {
        "vid": "0x4A4A",
        "pid": "0xBA11",
        "max_power": 100,
        "device_version": "0.0.1"
    },
    "ws2812": {
        "pin": "GP14",
        "driver": "vendor"
    },
    "features": {
        "bootmagic": false,
        "mousekey": true,
        "extrakey": false,
        "console": false,
        "command": false,
        "nkro": false,
        "backlight": false,
        "combo": false,
        "leader": false,
        "dynamic_macro": false,
        "os_detection": false,
        "caps_word": false,
        "rgblight": true,
        "programmable_button": false,
        "audio": false,
        "raw": true,
        "pointing_device": true
    },
    "rgblight": {
        "led_count": 6,
        "driver": "ws2812",
        "sleep": true,
        "animations": {
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "knight": true,
            "christmas": true,
            "static_gradient": true,
            "rgb_test": true,
            "alternating": true,
            "twinkle": true
        }
    },
    "layouts": {
        "LAYOUT_1x5": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 },
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [0, 3], "x": 3, "y": 0 },
                { "matrix": [0, 4], "x": 4, "y": 0 }
            ]
        }
    },
    "matrix_pins": {
        "cols": ["GP12", "GP11", "GP10", "GP9", "GP8"],
        "rows": ["GP13"]
    },
    "diode_direction": "COL2ROW",
    "build": {
        "lto": true
    }
}
